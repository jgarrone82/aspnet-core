@*
@model List<BaseSchoolObj>
*@

<table class="table table-dark table-hover table-sm">
    <thead>
        <tr>
            <th scope="col">Name </th>
            <th scope="col">Id </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.UniqueId</td>
            </tr>                
        }
    </tbody>
</table>